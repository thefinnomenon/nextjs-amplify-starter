{
  "name": "nextjs-amplify-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:updateSnapshots": "jest -u",
